<?xml version="1.0" encoding="UTF-8"?>
<config>
    <node>
        <id>1</id>
    </node>

    <redis>
        <host>"127.0.0.1"</host>
        <port>6380</port>
        <db>0</db>
    </redis>

    <mongodb>
        <host>"127.0.0.1"</host>
        <port>27017</port>
        <db>"sanguo"</db>
    </mongodb>

    <cache>
        <seconds>3600</seconds>
    </cache>

    <battle>
        <log>false</log>
    </battle>


    <!-- below are common settings for all nodes -->
    <!-- which means all nodes must keep the same settings -->

    <crypto>
        <key>"1234567890abcdef"</key>
        <prefix>"ok"</prefix>
    </crypto>

    <hub>
        <host>"127.0.0.1"</host>
        <port>8020</port>
    </hub>

    <log>
        <host>"127.0.0.1"</host>
        <port>10099</port>
    </log>
</config>

